<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.property.dao.ServiceKindDao">
	<select id="selectById" resultType="ServiceKind">
		SELECT
		`id`,
		`name`
		FROM
		`property`.`servicekind` where id =#{id}
	</select>
	<select id="selectAll" resultType="ServiceKind">
		SELECT
		`id`,
		`name`
		FROM
		`property`.`servicekind`
	</select>
</mapper>
  